{
  "name": "trips",
  "handler": "trips/handler.trips",
  "runtime": "nodejs",
  "description": "Management of Trips",
  "timeout": 10,
  "memorySize": 1024,
  "endpoints": [
    {
      "path": "/{vehicle_id}/trips",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.path.vehicle_id": "method.request.path.vehicle_id",
        "integration.request.querystring.page": "method.request.querystring.page"
      },
      "requestTemplates": {
        "application/json": "{ \"type\": \"trips\", \"page\": \"$input.params('page')\", \"vehicle_id\" : \"$input.params('vehicle_id')\", \"httpmethod\" : \"GET\" }"
      },
      "responses": {
        "400": {
          "selectionPattern": "^\\[BadRequest\\].*",
          "statusCode": "400"
        },
        "403": {
          "selectionPattern": "^\\[Forbidden\\].*",
          "statusCode": "403"
        },
        "404": {
          "selectionPattern": "^\\[NotFound\\].*",
          "statusCode": "404"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    },
    {
      "path": "/{vehicle_id}/trips/{id}",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{ \"type\": \"trips\", \"vehicle_id\" : \"$input.params('vehicle_id')\", \"id\" : \"$input.params('id')\", \"httpmethod\" : \"GET\" }"
      },
      "responses": {
        "400": {
          "selectionPattern": "^\\[BadRequest\\].*",
          "statusCode": "400"
        },
        "403": {
          "selectionPattern": "^\\[Forbidden\\].*",
          "statusCode": "403"
        },
        "404": {
          "selectionPattern": "^\\[NotFound\\].*",
          "statusCode": "404"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ]
}