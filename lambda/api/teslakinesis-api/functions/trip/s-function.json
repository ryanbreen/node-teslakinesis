{
  "name": "trip",
  "handler": "trip/handler.trips",
  "runtime": "nodejs",
  "description": "Management of Trips",
  "timeout": 10,
  "memorySize": 1024,
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "trip/{vehicle_id}",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{ \"vehicle_id\" : \"$input.params('vehicle_id')\", \"httpmethod\" : \"GET\" }"
      },
      "responses": {
        "400": {
          "selectionPattern": "^\\[BadRequest\\].*",
          "statusCode": "400"
        },
        "403": {
          "selectionPattern": "^\\[Forbidden\\].*",
          "statusCode": "403"
        },
        "404": {
          "selectionPattern": "^\\[NotFound\\].*",
          "statusCode": "404"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    },
    {
      "path": "trip/{id}",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{ \"vechicle_id\" : \"$input.params('vehicle_id')\", \"id\" : \"$input.params('id')\", \"httpmethod\" : \"GET\" }"
      },
      "responses": {
        "400": {
          "selectionPattern": "^\\[BadRequest\\].*",
          "statusCode": "400"
        },
        "403": {
          "selectionPattern": "^\\[Forbidden\\].*",
          "statusCode": "403"
        },
        "404": {
          "selectionPattern": "^\\[NotFound\\].*",
          "statusCode": "404"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ]
}